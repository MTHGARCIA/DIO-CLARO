package main.java.list.operacoesBasicas;

public class Item {
	
	private String nome;
	private double preco;
	private int quant;
	
	public Item (String nome, double preco, int quantidade) {
		this.nome = nome;
		this.preco = preco;
		this.quant = quant;
	}

	public String getNome() {
		return nome;
	}

	

	public double getPreco() {
		return preco;
	}

	

	public int getQuant() {
		return quant;
	}

		

}
